{
  "name": "exif-frame-web",
  "version": "1.0.0",
  "description": "写真にフレームを追加しExif情報を表示・編集するウェブアプリです",
  "main": "script.js",
  "scripts": {
    "lint:html": "markuplint \"*.html\"",
    "lint:html --fix": "markuplint \"*.html\" --fix",
    "lint:css": "stylelint \"*.css\"",
    "lint:css --fix": "stylelint \"*.css\" --fix",
    "lint:eslint": "eslint \"*.js\"",
    "lint:eslint --fix": "eslint \"*.js\" --fix",
    "lint:prettier": "prettier --write .",
    "lint": "run-p lint:*",
    "prepare": "husky install"
  },
  "lint-staged": {
    "*.js": "eslint",
    "*.css": "stylelint",
    "*.html": "markuplint"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ry0y4n/ExifFrame.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/ry0y4n/ExifFrame/issues"
  },
  "homepage": "https://github.com/ry0y4n/ExifFrame#readme",
  "devDependencies": {
    "eslint": "^8.48.0",
    "husky": "^8.0.0",
    "lint-staged": "^14.0.1",
    "markuplint": "^3.12.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.0.3",
    "stylelint": "15.10.3",
    "stylelint-config-recess-order": "^4.3.0",
    "stylelint-config-standard": "34.0.0"
  }
}
